<!DOCTYPE html>
<html lang="en">
<%- include("../partials/head.ejs") %>

    <body>
        <%- include("../partials/header.ejs") %>
            <main>
                <div class="container-fluid"> 
                    <div class="container text-center">
                        <div class="row align-items-start">
                            <div class="col">
                             <% for (fiber of fibers) { %>
                                 <article>
                                    <a href="/fibers/<%= fiber._id %>">
                           
                                         <h2> 
                                              <%= fiber.fiberID %> - <%= fiber.isItWorking ? "Usable" : "Already In Used" %>
                                         </h2>
                                     </a>
                                 </article>

                             <% } %>
                            </div>
                        </div>
                    </div>
                </div>
            </main>
    </body>

</html>